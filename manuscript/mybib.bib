@article{steiger2013PBS,
  title = {When the Sun Never Sets: Diverse Activity Rhythms under Continuous Daylight in Free-Living Arctic-Breeding Birds},
  shorttitle = {When the Sun Never Sets},
  author = {Steiger, Silke S. and Valcu, Mihai and Spoelstra, Kamiel and Helm, Barbara and Wikelski, Martin and Kempenaers, Bart},
  year = {2013},
  month = aug,
  volume = {280},
  pages = {20131016},
  issn = {1471-2954},
  doi = {10.1098/rspb.2013.1016},
  abstract = {Circadian clocks are centrally involved in the regulation of daily behavioural and physiological processes. These clocks are synchronized to the 24 h day by external cues (Zeitgeber), the most important of which is the light-dark cycle. In polar environments, however, the strength of the Zeitgeber is greatly reduced around the summer and winter solstices (continuous daylight or continuous darkness). How animals time their behaviour under such conditions has rarely been studied in the wild. Using a radio-telemetry-based system, we investigated daily activity rhythms under continuous daylight in Barrow, Alaska, throughout the breeding season in four bird species that differ in mating system and parental behaviour. We found substantial diversity in daily activity rhythms depending on species, sex and breeding stage. Individuals exhibited either robust, entrained 24 h activity cycles, were continuously active (arrhythmic) or showed 'free-running' activity cycles. In semipalmated sandpipers, a shorebird with biparental incubation, we show that the free-running rhythm is synchronized between pair mates. The diversity of diel time-keeping under continuous daylight emphasizes the plasticity of the circadian system, and the importance of the social and life-history context. Our results support the idea that circadian behaviour can be adaptively modified to enable species-specific time-keeping under polar conditions.},
  file = {/Users/erikhedlin/Zotero/storage/V28IX2TY/Steiger et al_2013_When the sun never sets.pdf},
  journal = {Proceedings. Biological Sciences},
  keywords = {Activity Cycles,Alaska,Animals,Arctic,Arctic Regions,arrhythmic activity,Birds,Calidris,circadian clock,Circadian Clocks,Circadian Rhythm,continuous daylight,Environmental Monitoring,Female,Male,mating system,Melatonin,Photoperiod,Sexual Behavior; Animal,Songbirds,Species Specificity,Telemetry,Time Factors},
  language = {eng},
  number = {1764},
  pmcid = {PMC3712422},
  pmid = {23782884}
}

@article{stirling2012GCB,
