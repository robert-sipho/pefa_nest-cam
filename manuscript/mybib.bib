@article{sicurella2015BS,
  title = {Weather Conditions, Brood Size and Hatching Order Affect {{Common Swift}} {{{\emph{Apus}}}}{\emph{ Apus}} Nestlings' Survival and Growth},
  author = {Sicurella, Beatrice and Caffi, Mario and Caprioli, Manuela and Rubolini, Diego and Saino, Nicola and Ambrosini, Roberto},
  year = {2015},
  month = jan,
  volume = {62},
  pages = {64--77},
  issn = {0006-3657, 1944-6705},
  doi = {10.1080/00063657.2014.989193},
  abstract = {Methods Nestlings were weighed at regular intervals and molecularly sexed. Growth patterns were described using a double-Richards growth curve, i.e. a growth curve that also accounted for the prefledging mass loss. We also identified the Linear Growth Phase (i.e. when nestlings grew at the fastest rate). Results Mortality increased with hatching order and decreased with brood size. Mortality was highest for fourth-hatched nestlings, none of which survived. Nestlings hatched late in the season grew faster, experienced the fastest growth rate at a younger age, but reached a lower maximum weight than nestlings hatched early in the season. Nestlings from large broods reached a lower maximum weight than those of small ones. Nestlings gained more mass when temperature was slightly higher than that expected from the seasonal temperature trend, and on rainy and windy days. Conclusion Common Swift nestlings' growth, but not survival, was affected by meteorological conditions, with moderate rainfall, moderate winds and high temperatures favouring greater mass gain. Higher intra-brood competition resulted in slower growth and lower maximum weight. Late-hatched nestlings grew faster but reached a lower maximum weight and a lower pre-fledging mass, possibly suggesting that late-hatched nestlings adaptively tune their growth pattern to the approaching onset of migration to their African wintering quarters. Our results emphasize the importance of exploring weather effects on the breeding biology of a species in different portions of its distribution range that are characterized by contrasting climatic conditions.},
  file = {/Volumes/GoogleDrive/My Drive/NuWCRU/zotero-library/Sicurella et al/Sicurella et al_2015_Weather conditions, brood size and hatching order affect Common Swift iApus.pdf;/Volumes/GoogleDrive/My Drive/NuWCRU/zotero-library/Sicurella et al/Sicurella et al_2015_Weather conditions, brood size and hatching order affect Common Swift iApus2.pdf;/Volumes/GoogleDrive/My Drive/NuWCRU/zotero-library/Sicurella et al/Sicurella et al_2015_Weather conditions, brood size and hatching order affect Common Swift iApus3.pdf;/Volumes/GoogleDrive/My Drive/NuWCRU/zotero-library/Sicurella et al/Sicurella et al_2015_Weather conditions, brood size and hatching order affect Common Swift iApus4.pdf;/Volumes/GoogleDrive/My Drive/NuWCRU/zotero-library/Sicurella et al/Sicurella et al_2015_Weather conditions, brood size and hatching order affect Common Swift iApus5.pdf;/Volumes/GoogleDrive/My Drive/NuWCRU/zotero-library/Sicurella et al/Sicurella et al_2015_Weather conditions, brood size and hatching order affect Common Swift iApus6.pdf},
  journal = {Bird Study},
  language = {en},
  number = {1}
}

@article{signer2019EE,
  title = {Animal Movement Tools ( Amt ): {{R}} Package for Managing Tracking Data and Conducting Habitat Selection Analyses},
  shorttitle = {Animal Movement Tools ( Amt )},
  author = {Signer, Johannes and Fieberg, John and Avgar, Tal},
  year = {2019},
  month = jan,
  volume = {9},
  pages = {880--890},
  issn = {20457758},
  doi = {10.1002/ece3.4823},
  abstract = {Advances in tracking technology have led to an exponential increase in animal location data, greatly enhancing our ability to address interesting questions in movement ecology, but also presenting new challenges related to data management and analysis. Step-selection functions (SSFs) are commonly used to link environmental covariates to animal location data collected at fine temporal resolution. SSFs are estimated by comparing observed steps connecting successive animal locations to random steps, using a likelihood equivalent of a Cox proportional hazards model. By using common statistical distributions to model step length and turn angle distributions, and including habitat- and movement-related covariates (functions of distances between points, angular deviations), it is possible to make inference regarding habitat selection and movement processes or to control one process while investigating the other. The fitted model can also be used to estimate utilization distributions and mechanistic home ranges. Here, we present the R package amt (animal movement tools) that allows users to fit SSFs to data and to simulate space use of animals from fitted models. The amt package also provides tools for managing telemetry data. Using fisher (Pekania pennanti) data as a case study, we illustrate a four-step approach to the analysis of animal movement data, consisting of data management, exploratory data analysis, fitting of models, and simulating from fitted models.},
  file = {/Volumes/GoogleDrive/My Drive/NuWCRU/zotero-library/Signer et al/Signer et al_2019_Animal movement tools ( amt ).pdf},
  journal = {Ecology and Evolution},
  language = {en},
  number = {2}
}

