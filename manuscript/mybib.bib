@article{kusunoki2015PS,
  title = {Future Changes in Precipitation Intensity over the {{Arctic}} Projected by a Global Atmospheric Model with a 60-Km Grid Size},
  author = {Kusunoki, Shoji and Mizuta, Ryo and Hosaka, Masahiro},
  year = {2015},
  month = sep,
  volume = {9},
