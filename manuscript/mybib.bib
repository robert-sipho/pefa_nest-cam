  shorttitle = {Is Natural Hatching Asynchrony Optimal?},
  author = {Merkling, Thomas and Agdere, Lena and Albert, Elise and Durieux, Romain and Hatch, Scott A. and Danchin, Etienne and Blanchard, Pierrick},
