}

@article{dell2005EJE,
  title = {Climate Change and the Effect of Increasing Spring Temperatures on Emergence Dates of the Butterfly {{Apatura}} Iris ({{Lepidoptera}}: {{Nymphalidae}})},
  shorttitle = {Climate Change and the Effect of Increasing Spring Temperatures on Emergence Dates of the Butterfly {{Apatura}} Iris ({{Lepidoptera}}},
  author = {Dell, Dennis and Sparks, Tim H. and Dennis, Roger L.H.},
  year = {2005},
  month = may,
  volume = {102},
